{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "rust-labeler",
  "version": "0.2.0",
  "identifier": "dev.potuznik.rust-labeler",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "rust-labeler",
        "width": 1100,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.ico"
    ]
  }
}
